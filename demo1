/*应用于多项条件排序*/
#include <iostream>
#include <cstdio>
#include <algorithm>
using namespace std;
struct point
{
    int v,s;
}p[];
bool cmp(point p1,point p2)
{
    if (p1.x!=p2.x)
       return p1.x>p2.x;
    else
       return p1.v<p2.v;
}
int main()
{
    int n,i;
    scanf("%d",&n);
    for(i=0;i<n;i++)
        scanf("%d%d",&p.s,&p.v);
    sort(p,p+n,cmp);
}
